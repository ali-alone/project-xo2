<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm ="clr-namespace:project_xo2.ViewModel" 
                    xmlns:view ="clr-namespace:project_xo2.View">
    <DataTemplate DataType="{x:Type vm:MainWM}">
        <view:Main/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:StudentWM}">
        <view:Studenet/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TeacherWM}">
        <view:Teacher/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CourseWM}">
        <view:Course/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:STC}">
        <view:STC/>
    </DataTemplate>

</ResourceDictionary>