<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm ="clr-namespace:project_xo2.ViewModel" 
                    xmlns:vmm ="clr-namespace:project_xo2.user" 
                    xmlns:view="clr-namespace:project_xo2.View" 
                    xmlns:user="clr-namespace:project_xo2.user">
    <DataTemplate DataType="{x:Type vm:MainWM}">
        <view:Main/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:StudentWM}">
        <view:Studenet/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TeacherWM}">
        <view:Teacherv/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CourseWM}">
        <view:Coursev/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:STCWM}">
        <view:STCv/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmm:UserWM}">
        <user:UserPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmm:UTeacherWM}">
        <user:UTeacher/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vmm:UCourseWM}">
        <user:UCorse/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vmm:USTCWM}">
        <user:USTC/>
    </DataTemplate>



</ResourceDictionary>